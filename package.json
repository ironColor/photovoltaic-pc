{
  "name": "qin-yan-jin",
  "version": "6.0.0",
  "private": true,
  "description": "一个简单的React+AntDesign5.0+AntDesignPro项目",
  "scripts": {
    "start:dev": "cross-env REACT_APP_ENV=dev MOCK=none UMI_ENV=dev max dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev max dev",
    "analyze": "cross-env ANALYZE=1 max build",
    "build": "max build",
    "gh-pages": "npm run build && gh-pages -d dist -e [远端 git 目录] -r [远端 git 地址] -b [远端 git 分支]",
    "postinstall": "max setup",
    "lint": "npm run lint:js && npm run lint:prettier && tsc --noEmit",
    "lint:fix": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src ",
    "lint:js": "eslint --cache --ext .js,.jsx,.ts,.tsx --format=pretty ./src",
    "lint:prettier": "prettier -c --write \"**/**.{js,ts,jsx,tsx,less,css,json,md,html,scss,yml}\" --end-of-line auto",
    "prepare": "husky install",
    "prettier": "prettier -c --write \"**/**.{js,ts,jsx,tsx,less,css,json,md,html,scss,yml}\""
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "eslint --ext .js,.jsx,.ts,.tsx"
    ],
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@amap/amap-jsapi-loader": "^1.0.1",
    "@amap/amap-jsapi-types": "^0.0.13",
    "@ant-design/icons": "^5.2.6",
    "@ant-design/pro-components": "^2.6.43",
    "@ant-design/use-emotion-css": "1.0.4",
    "@emotion/css": "^11.11.2",
    "@umijs/route-utils": "^2.2.2",
    "ahooks": "^3.7.8",
    "antd": "^5.20.2",
    "classnames": "^2.3.2",
    "gcoord": "^1.0.5",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "rc-menu": "^9.8.2",
    "rc-util": "^5.27.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.5",
    "@types/history": "^4.7.11",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/react-helmet": "^6.1.11",
    "@umijs/fabric": "^2.14.1",
    "@umijs/lint": "^4.1.0",
    "@umijs/max": "^4.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "lint-staged": "^10.5.4",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "umi-presets-pro": "^2.0.2"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
